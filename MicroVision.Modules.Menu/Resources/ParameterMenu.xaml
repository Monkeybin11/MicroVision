<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <MenuItem x:Key="ProfileFileMenuItem" Header="Profile _File">
        <MenuItem Header="_Save" Command="{Binding SaveCommand}"></MenuItem>
        <MenuItem Header="Save _As" Command="{Binding SaveAsCommand}"></MenuItem>
        <MenuItem Header="_Load" Command="{Binding LoadCommand}"></MenuItem>
    </MenuItem>
</ResourceDictionary>